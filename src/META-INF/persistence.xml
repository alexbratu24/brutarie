<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="BrutariePU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>model.Users</class>
    <class>model.TypesEmps</class>
    <class>model.ToCollect</class>
    <class>model.Returns</class>
    <class>model.Collected</class>
    <class>model.Employees</class>
    <class>model.Products</class>
    <class>model.ClientProdPrices</class>
    <class>model.OrderProducts</class>
    <class>model.Clients</class>
    <class>model.Routes</class>
    <class>model.Order1</class>
    <class>model.Productions</class>
    <class>model.ReturnedProducts</class>
    <class>model.RouteClients</class>
    <class>model.HistoryLogs</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:sqlserver://localhost:1433;databaseName=Brutarie"/>
      <property name="javax.persistence.jdbc.user" value="sa"/>
      <property name="javax.persistence.jdbc.driver" value="com.microsoft.sqlserver.jdbc.SQLServerDriver"/>
      <property name="javax.persistence.jdbc.password" value="root"/>
    </properties>
  </persistence-unit>
</persistence>
